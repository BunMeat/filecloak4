{
    "version": 2,
    "builds": [
      {
        "src": "filecloak/api/index.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "filecloak/src/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "filecloak/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.mjs"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/filecloak/build/index.html"
      }
    ]
  }
  