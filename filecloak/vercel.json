{
  "version": 2,
  "builds": [
    {
      "src": "/src/index.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "webpack.config.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/routes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.mjs"
    }
  ]
}
